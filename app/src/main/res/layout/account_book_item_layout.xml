<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/AccountBookItemLayoutGuidelineV5"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.5" />

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/AccountBookItemLayoutGuidelineV75"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.75" />

	<TextView
		android:id="@+id/AccountBookItemLayoutName"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="-"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/AccountBookItemLayoutTime"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:text="-"
		android:textAlignment="viewStart"
		app:layout_constraintEnd_toEndOf="@id/AccountBookItemLayoutGuidelineV5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/AccountBookItemLayoutName" />

	<TextView
		android:id="@+id/AccountBookItemLayoutSite"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:text="-"
		android:textAlignment="center"
		app:layout_constraintEnd_toEndOf="@id/AccountBookItemLayoutGuidelineV75"
		app:layout_constraintStart_toEndOf="@id/AccountBookItemLayoutTime"
		app:layout_constraintTop_toBottomOf="@id/AccountBookItemLayoutName" />

	<TextView
		android:id="@+id/AccountBookItemLayoutAccount"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:text="-"
		android:textAlignment="center"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@id/AccountBookItemLayoutSite"
		app:layout_constraintTop_toBottomOf="@id/AccountBookItemLayoutName" />

	<TextView
		android:id="@+id/AccountBookItemLayoutOriginalPrice"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:text="-"
		android:textAlignment="viewEnd"
		app:layout_constraintEnd_toEndOf="@id/AccountBookItemLayoutGuidelineV5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/AccountBookItemLayoutTime" />

	<TextView
		android:id="@+id/AccountBookItemLayoutPrice"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:text="-"
		android:textAlignment="viewEnd"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@id/AccountBookItemLayoutOriginalPrice"
		app:layout_constraintTop_toBottomOf="@id/AccountBookItemLayoutSite" />

</androidx.constraintlayout.widget.ConstraintLayout>